<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tecsun.sisp.net.modules.dao.common.CommQueryDao">

    <!--查询数据字典表-->
    <select id="getDictionaryById" parameterType="java.lang.String"
            resultType="com.tecsun.sisp.net.modules.entity.response.DictVO">
        select  * from T_DICTIONARY t where GROUPID = #{groupid}
    </select>


